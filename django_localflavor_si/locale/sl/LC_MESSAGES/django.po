#: forms.py:24
msgid "This field should contain exactly 13 digits."
msgstr "To polje mora vsebovati natanko 13 števil."

#: forms.py:25
msgid "The first 7 digits of the EMSO must represent a valid past date."
msgstr "Prvih 7 števk EMŠO števila mora biti veljaven pretekli datum."

#: forms.py:26
msgid "The EMSO is not valid."
msgstr "EMŠO ni veljaven."

#: forms.py:86
msgid "Enter a valid tax number in form SIXXXXXXXX"
msgstr "Vnesite veljavno davčno številko v zapisu SIXXXXXXXX"

#: forms.py:151
msgid "Enter phone number in form +386XXXXXXXX or 0XXXXXXXX."
msgstr "Vnesite telefonsko številko v zapisu +386XXXXXXXX ali 0XXXXXXXX."
